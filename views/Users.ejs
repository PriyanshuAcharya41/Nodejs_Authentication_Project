<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.5/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="bg-dark text-light">
    <h1>Welcome to Full Stack Authentication Project</h1>
    <div class="container text-center">
        <% user.forEach((user) => { %>
            <div class="bg-dark text-light my-3" style="border: solid 2px;">
                <img src="<%= user.profileImg %>" style="height: 200px; width: 200px; border-radius: 50%;" alt="">
                <h4><%= user.name %></h4>
                <h5><%= user.email %></h5>
            </div>
        <% }); %>
    </div>
    <div class=" text-center my-5">
        <a href="/login" class="btn btn-warning">
            Logout
        </a>
        <a href="/users" class="btn btn-primary">
            All Users
        </a>
    </div>
</body>
</html>